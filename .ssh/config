Host eniac
    HostName eniac.seas.upenn.edu
    User mannd
    IdentityFile ~/.ssh/id_rsa

Host spec* big*
    User mannd
    ProxyCommand ssh eniac -W %h:%p
    IdentityFile ~/.ssh/id_rsa

Host github.com
    IdentityFile ~/.ssh/github

Host ec2*compute-1.amazonaws.com
    IdentityFile ~/.ssh/boontorrent.pem
    User ubuntu
